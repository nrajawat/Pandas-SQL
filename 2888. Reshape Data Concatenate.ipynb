   "source": [
    "import pandas as pd\n",
    "\n",
    "def concatenateTables(df1: pd.DataFrame, df2: pd.DataFrame) -> pd.DataFrame:\n",
    "    \n",
    "    concate = pd.concat([df1, df2], ignore_index = True)\n",
    "    return concate\n",
    "\n"
   ]
